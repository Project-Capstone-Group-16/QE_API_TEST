{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "message": {
      "type": "string"
    },
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "name": {
            "type": "string"
          },
          "city": {
            "type": "string"
          },
          "province": {
            "type": "string"
          },
          "status": {
            "type": "string"
          },
          "capacity": {
            "type": "integer",
            "enum": [
              0
            ]
          },
          "favorite": {
            "type": "integer"
          },
          "image_url": {
            "type": "string"
          }
        }
      }
    }
  }
}